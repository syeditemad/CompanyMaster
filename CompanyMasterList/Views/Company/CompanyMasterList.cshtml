@model IEnumerable<CompanyMasterList.Models.CompanyMaster>

@{
    ViewBag.Title = "CompanyMasterList";
}

<h2>CompanyMasterList</h2>

<p>
    @Html.ActionLink("Create New", "AddEditCompnayMasterList", "Company", new {@class="btn btn-primary"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Companyname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Companyname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "AddEditCompnayMasterList","Company", new {@class="btn btn-info"}, new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
